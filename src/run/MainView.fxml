<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="run.MainViewController">
   <children>
      <Button fx:id="chooseFileButton" layoutX="49.0" layoutY="60.0" mnemonicParsing="false" onAction="#handleNewFile" text="Choose Your File" />
      <Label layoutX="21.0" layoutY="38.0" text="Please Choose your Json File" />
      <Label fx:id="displayLoc" layoutX="196.0" layoutY="55.0" prefHeight="36.0" prefWidth="294.0" />
      <Button fx:id="run" layoutX="411.0" layoutY="221.0" mnemonicParsing="false" onAction="#handleRun" prefHeight="36.0" prefWidth="106.0" text="Run">
         <font>
            <Font name="Avenir Heavy" size="24.0" />
         </font>
      </Button>
      <ProgressBar layoutX="20.0" layoutY="247.0" prefHeight="36.0" prefWidth="335.0" progress="0.0" />
      <Label layoutX="21.0" layoutY="124.0" text="Please enter the file name to save the output" />
      <TextField fx:id="outputFileName" editable="false" layoutX="244.0" layoutY="150.0" prefHeight="26.0" prefWidth="264.0" />
      <Button fx:id="outputButton" layoutX="49.0" layoutY="150.0" mnemonicParsing="false" onAction="#handleOutputFile" text="Provide new file name" />
   </children>
</AnchorPane>
